{
	"name": "coding-in-the-dark",
	"displayName": "Coding in the Dark",
	"description": "Blind HTML/CSS... competitively",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:coding-in-the-dark.startPlaying"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "coding-in-the-dark.startPlaying",
				"title": "Code in the Dark!"
			}
		],
		"configuration": {
			"title": "Coding in the Dark",
			"properties": {
				"coding-in-the-dark.url": {
					"type": "string",
					"default": "http://localhost:8080",
					"pattern": "^https?:\\/\\/[a-zA-Z\\.\\/0-9:]+$",
					"description": "Code submission URL"
				},
				"coding-in-the-dark.time-limit": {
					"type": "integer",
					"default": 900,
					"minimum": 60,
					"maximum": 3600,
					"description": "Time given per game (seconds)"
				}
			}
		}
	},
	"scripts": {
		"lint": "eslint ."
	},
	"devDependencies": {
		"eslint": "^7.15.0"
	},
	"dependencies": {
		"handlebars": "^4.7.6",
		"node-fetch": "^2.6.1",
		"socket.io-client": "^3.1.0"
	}
}
